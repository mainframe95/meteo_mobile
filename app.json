{
    "expo": {
        "name": "meteo",
        "slug": "meteo",
        "platforms": [
            "ios",
            "android",
            "web"
        ],
        "sdkVersion": "34.0.0",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        }
    }
}